{
	"bsonType": "object",
	"required": ["y_m", "day"],
	"permission": {
		"read": true,
		"create": "'AUDITOR' in auth.role",
		"update": "'AUDITOR' in auth.role",
		"delete": "'AUDITOR' in auth.role"
	},
	"properties": {
		"_id": {
			"description": "预约ID，系统自动生成"
		},
		"eid": {
			"description": "设备ID",
			"bsonType": "string",
			"foreignKey": "srt-equip._id"
		},
		"y_m": {
			"description": "年份_月份",
			"bsonType": "string",
			"pattern": "^[0-9]{4}-[0-9]{2}$"
		},
		"day":{
			"description": "日期状压",
			"bsonType": "int"
		}
	}
}

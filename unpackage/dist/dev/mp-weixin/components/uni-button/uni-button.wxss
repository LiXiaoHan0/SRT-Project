/* 字体家族 */
@keyframes ripple-f2d01f38 {
0% {
    background-image: radial-gradient(circle at center, var(--hover-color) 0%, var(--bg-color));
}
10% {
    background-image: radial-gradient(circle at center, var(--hover-color) 10%, var(--bg-color));
}
20% {
    background-image: radial-gradient(circle at center, var(--hover-color) 20%, var(--bg-color));
}
30% {
    background-image: radial-gradient(circle at center, var(--hover-color) 30%, var(--bg-color));
}
40% {
    background-image: radial-gradient(circle at center, var(--hover-color) 40%, var(--bg-color));
}
50% {
    background-image: radial-gradient(circle at center, var(--hover-color) 50%, var(--bg-color));
}
60% {
    background-image: radial-gradient(circle at center, var(--hover-color) 60%, var(--bg-color));
}
70% {
    background-image: radial-gradient(circle at center, var(--hover-color) 70%, var(--bg-color));
}
80% {
    background-image: radial-gradient(circle at center, var(--hover-color) 80%, var(--bg-color));
}
90% {
    background-image: radial-gradient(circle at center, var(--hover-color) 90%, var(--bg-color));
}
100% {
    background-image: radial-gradient(circle at center, var(--hover-color) 100%, var(--bg-color));
}
}
.buttons.data-v-f2d01f38 {
  padding: 8px 16px;
  border-radius: 10px;
  box-shadow: 0 3px 8px rgba(0, 0, 0, 0.3);
  border: var(--bord-color) solid 3px;
  color: #FFFFFF;
  text-align: center;
  font: bold 18px "Roboto", sans-serif;
  background: var(--bg-color);
  transform: scale(1);
  transition: transform 50ms;
  transition-timing-function: ease-in-out;
}
.buttons-hover.data-v-f2d01f38 {
  transform: scale(0.98);
  animation: ripple-f2d01f38 100ms ease-in-out forwards;
}